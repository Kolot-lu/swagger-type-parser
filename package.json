{
  "name": "swagger-type-parser",
  "version": "1.0.0",
  "description": "CLI tool to generate TypeScript types from OpenAPI/Swagger specifications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "swagger-type-parser": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "type-generation",
    "api",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "ts-jest": "^29.1.1"
  },
  "optionalDependencies": {
    "prettier": "^3.1.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}

